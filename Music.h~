#include <iostream>
#include "Media.h"
#include <cstring>
class Music : public Media
{
 public:
  Music(char*, int, char*, float, char*);
  ~Music();
  char* getArtist();
  float getDuration();
  char* getPublisher();
  int getType();
 private:
  char* artist;
  float duration;
  char* publisher;
};
